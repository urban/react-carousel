{
  "name": "react-carousel",
  "version": "1.0.0",
  "description": "Carousel built in React.js",
  "author": "Urban Faubion <urban.faubion@gmail.com>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/urban/react-carousel.git"
  },
  "readme": "README.md",
  "directories": {
    "lib": "./lib",
    "example": "./examples"
  },
  "files": [
    "./lib",
    "./README.md",
    "./LICENSE"
  ],
  "main": "./lib/react-carousel.js",
  "scripts": {
    "test": "jest",
    "release": "gulp",
    "dev": "cd ./examples && npm start"
  },
  "jest": {
    "testDirectoryName": "test",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ]
  },
  "dependencies": {
    "babel": "^4.4.5",
    "react": "^0.13.0-rc2"
  },
  "devDependencies": {
    "babel-core": "^4.4.5",
    "babel-jest": "^4.0.0",
    "babel-runtime": "^4.7.2",
    "babelify": "^5.0.3",
    "browserify": "^6.3.4",
    "browserify-shim": "^3.8.3",
    "envify": "^3.2.0",
    "file-loader": "^0.8.1",
    "gulp": "^3.8.11",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-uglify": "^1.1.0",
    "jest": "^0.1.37",
    "jest-cli": "^0.2.2",
    "merge-stream": "^0.1.6",
    "through2": "^0.6.3",
    "uglify-js": "^2.4.16",
    "url-loader": "^0.5.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "keywords": [
    "react",
    "react-ui",
    "react-element"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "experimental": true,
          "loose": "all"
        }
      ],
      "browserify-shim",
      [
        "envify",
        {
          "detect-globals": true
        }
      ]
    ]
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "reactInitOptions": {
    "entryFile": "Carousel.jsx",
    "exportVar": "Carousel"
  }
}
